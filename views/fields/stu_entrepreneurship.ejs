<%- include ("../partials/header") %>


  <div class="container-fluid">

    <section id="form">
      <form id="myForm" class="needs-validation" novalidate action="/students/stu_entrepreneurship">
        <div class="container-fluid">

          <!-- Header Section -->

          <div class="row data-row">
            <div class="col-md-8 mb-3">
              <h4 class="mb-3 title">Entrepreneurship Details Data Entry</h4>
            </div>
            <div class="col-md-4 mb-3 report">
              <a href="/students/search" class="btn btn-sm btn-primary shadow-sm"><i
                  class="fas fa-download fa-sm text-white-50"></i> Generate Report</a>
            </div>
          </div>
          <div class="row data-row">
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">RC-ID</label>
                <input type="text" name="rcid" class="form-control" required>
                <div class="invalid-feedback">
                  Valid RC-ID is required.
                </div>
              </div>
            </div>
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">Company Type</label>
                <input type="text" name="companyType" class="form-control" required>
                <div class="invalid-feedback">
                  Valid company type is required.
                </div>
              </div>
            </div>
          </div>
          <div class="row data-row">
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">Company Name & Address</label>
                <input type="text" name="companyNameAddress" class="form-control" required>
                <div class="invalid-feedback">
                  Valid company name and address is required.
                </div>
              </div>
            </div>
            <div class="col-md-6 mb-3">
              <label class="required">Date of Establishment</label>
              <input type="date" name="date" class="form-control" onchange="setDate()" required>
            </div>
          </div>
          <div class="row data-row">
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label>Revenue Generated</label>
                <input type="text" name="revenue" class="form-control">
                <div class="invalid-feedback">
                  Valid revenue required.
                </div>
              </div>
            </div>
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">Objectives</label>
                <input type="text" name="objectives" class="form-control">
                <div class="invalid-feedback">
                  Valid objectives required.
                </div>
              </div>
            </div>
          </div>
          <div class="row data-row">
            <div class="col-md-12 mb-3">
              <div class="form-group">
                <label class="required">Benefits in terms of learning/Skill/Knowledge Obtained</label>
                <input type="text" name="benefits" class="form-control">
                <div class="invalid-feedback">
                  Valid skills required.
                </div>
              </div>
            </div>
          </div>

          <div class="row data-row">

            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">In association with</label>
                <select name="department" class="custom-select d-block w-100" required>
                  <option value="">Choose...</option>
                  <option>CSE</option>
                  <option>ISE</option>
                  <option>CIVIL</option>
                  <option>MECH</option>
                  <option>ECE</option>
                  <option>EEE</option>
                  <option>BS</option>
                  <option>MBA</option>
                  <option>MCA</option>
                  <option>TCE</option>
                </select>
                <div class="invalid-feedback">
                  Please select a valid department.
                </div>
              </div>
            </div>
            <div class="col-md-6 mb-3">
              <div class="form-group">
                <label class="required">COE</label>

                <!-- COE -->
                <%- include ("../partials/coe") %>

                  <div class="invalid-feedback">
                    Please provide a valid type.
                  </div>
              </div>


              <!-- <div class="col-md-6 mb-3">

              <label class="required">Submission Date</label>
              <input type="date" class="form-control" name="filterDate" required>

              <div class="invalid-feedback">
                Please provide a valid details.
              </div>
            </div> -->


            </div>
          </div>
          <%- include ("../partials/description") %>


            <div class="submit-button"><button class="btn btn-primary btn-lg btn-block buttons" type="submit"
                id="btnSubmit">
                Submit</button>
            </div>
        </div>



      </form>

    </section>

  </div>

  <!-- Footer -->

  <%- include ("../partials/footer") %>

    <script type="text/javascript" src="/js/textbox.js"></script>
    <script type="text/javascript" src="/js/date.js"></script>


    </body>

    </html>