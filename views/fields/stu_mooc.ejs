<%- include ("../partials/header") %>

    <div class="container-fluid">
        <section id="form">
            <form id="myForm" class="needs-validation" novalidate action="/students/stu_mooc">
                <div class="container-fluid">
                    <div class="row data-row">
                        <div class="col-md-8 mb-3">
                            <h4 class="mb-3 title">MOOC Data Entry</h4>
                        </div>
                        <div class="col-md-4 mb-3 report">
                            <a href="/students/search" class="btn btn-sm btn-primary shadow-sm"><i
                                    class="fas fa-download fa-sm text-white-50"></i> Generate Report</a>
                        </div>
                    </div>
                    <div class="row data-row">
                        <div class="col-md-6 mb-3">
                            <label class="required">USN</label>
                            <input type="text" name="usn" class="form-control" required>
                            <div class="invalid-feedback">
                                Valid USN is required.
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label class="required">Topic</label>
                            <input type="text" name="courseName" class="form-control" required>
                            <div class="invalid-feedback">
                                Valid course name is required.
                            </div>
                        </div>

                        <!-- <div class="col-md-6 mb-3">
                            <label class="required">Department</label>
                            <select name="dept" class="custom-select d-block w-100" required>
                                <option value="">Choose...</option>
                                <option>CSE</option>
                                <option>ISE</option>
                                <option>ECE</option>
                                <option>TCE</option>
                                <option>EEE</option>
                                <option>CIV</option>
                                <option>MECH</option>
                                <option>MBA</option>
                                <option>MCA</option>
                                <option>Basic Sciences</option>
                            </select>
                            <div class="invalid-feedback">
                                Valid department is required.
                            </div>
                        </div> -->
                    </div>
                    <div class="row data-row">
                        <div class="col-md-6 mb-3">
                            <label class="required">Department</label>
                            <select name="dept" class="custom-select d-block w-100" required>
                                <option value="">Choose...</option>
                                <option>CSE</option>
                                <option>ISE</option>
                                <option>ECE</option>
                                <option>TCE</option>
                                <option>EEE</option>
                                <option>CIV</option>
                                <option>MECH</option>
                                <option>MBA</option>
                                <option>MCA</option>
                                <option>Basic Sciences</option>
                            </select>
                            <div class="invalid-feedback">
                                Valid department is required.
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label class="required">Registered In</label>
                            <select name="agency" class="custom-select d-block w-100" onChange="changeFunc();"
                                id="selectBox" required>
                                <option value="">Choose...</option>
                                <option>NPTEL</option>
                                <option>Coursera</option>
                                <option>CMRIT</option>
                                <option value="others">Others</option>
                            </select>
                            <input name="agency" placeholder="Mention Here..." class="form-control other-text"
                                type="text" style="display: none" id="textboxes">
                            <div class="invalid-feedback">
                                Valid agency is required.
                            </div>
                        </div>
                    </div>
                    <div class="row data-row">
                        <div class="col-md-6 mb-3">
                            <label class="required">Start Date</label>
                            <input type="date" name="startDate" class="form-control" onchange="setDate()" required>
                            <div class="invalid-feedback">
                                Valid start date is required.
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label class="required">End Date</label>
                            <input type="date" class="form-control" name="endDate" required>
                            <div class="invalid-feedback">
                                Valid end date is required.
                            </div>

                        </div>
                    </div>
                    <div class="row data-row">
                        <div class="col-md-6 mb-3">
                            <label class="required">Applied for exam</label>
                            <select name="boolExam" class="custom-select d-block w-100" required>
                                <option value="">Choose...</option>
                                <option>Yes</option>
                                <option>No</option>
                            </select>
                            <div class="invalid-feedback">
                                Please select a valid option.
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label>COE</label>
                            <!-- COE -->
                            <%- include ("../partials/coe") %>

                        </div>
                    </div>




                    <%- include ("../partials/description") %>
                        <button class="btn btn-primary btn-lg btn-block buttons" type="submit">Submit</button>
                </div>
            </form>
        </section>


    </div>

    <!-- Footer -->
    <%- include ("../partials/footer") %>

        <script type="text/javascript" src="/js/textbox.js"></script>
        <script type="text/javascript" src="/js/date.js"></script>


        </body>

        </html>